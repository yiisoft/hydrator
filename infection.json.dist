{
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "text": "php:\/\/stderr",
        "stryker": {
            "report": "master"
        }
    },
    "mutators": {
        "@default": true,
        "global-ignoreSourceCodeByRegex": [
            ".*PHP_VERSION_ID.*"
        ],
        "FalseValue": {
            "ignoreSourceCodeByRegex": [
                ".*\\$hasMutable = false.*"
            ]
        }
    }
}
